---
import { siteData } from "site.config";
const { faqs } = siteData;
---

<div class="mt-10">
  <dl
    class="space-y-8 sm:grid sm:grid-cols-2 sm:gap-x-6 sm:gap-y-10 sm:space-y-0 lg:gap-x-10"
  >
    {
      faqs.map((faq) => (
        <div>
          <dt class="text-sm leading-4 font-semibold ">{faq.question}</dt>
          <dd class="mt-2 text-sm dark:text-gray-400 text-gray-600">
            {faq.answer}
          </dd>
        </div>
      ))
    }
  </dl>
</div>
